<script type="text/javascript">


	window.onload = function() {
		var cobj = document.getElementById("tt2").rows;
		cobj[0].style.background = "red";
	}
	function getCurDate() {
		var d = new Date();
		var week;
		switch (d.getDay()) {
		case 1:
			week = "星期一";
			break;
		case 2:
			week = "星期二";
			break;
		case 3:
			week = "星期三";
			break;
		case 4:
			week = "星期四";
			break;
		case 5:
			week = "星期五";
			break;
		case 6:
			week = "星期六";
			break;
		default:
			week = "星期天";
		}
		var years = d.getYear() + 1900;
		var month = add_zero(d.getMonth() + 1);
		var days = add_zero(d.getDate());
		var hours = add_zero(d.getHours());
		var minutes = add_zero(d.getMinutes());
		var seconds = add_zero(d.getSeconds());
		var ndate = years + "年" + month + "月" + days + "日 " + hours + ":"
				+ minutes + ":" + seconds + " " + week;
		divT.innerHTML = ndate;
	}
	function add_zero(temp) {
		if (temp < 10)
			return "0" + temp;
		else
			return temp;
	}
	setInterval("getCurDate()", 100);

	function checkRgieter() {
		var domain = document.getElementById("name").value;
		if (domain == "" || domain == null) {

			alert("表名不能为空");
			return false;
			}
	
		else {

			//document.getElementById("tijiao").disabled = true;
			document.getElementById("res").innerHTML = "生成中...";
			return true;
		}
			
	}



	function checkDomain() {

		var myXmlhttp;
		if (window.XMLHttpRequest) {

			myXmlhttp = new XMLHttpRequest();
		} else {
			myXmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
		}

		if (myXmlhttp) {
			var url = "checkDomain.action";
			var name = document.getElementById("name").value;
			var data = "name=" + name + "&time=" + new Date().getTime();

			myXmlhttp.open("POST", url, true);
			myXmlhttp.setRequestHeader("Content-type",
					"application/x-www-form-urlencoded");

			myXmlhttp.onreadystatechange = function() {
				if (myXmlhttp.readyState == 4 && myXmlhttp.status == 200) {
					var u = myXmlhttp.responseText;
					document.getElementById("info").innerHTML = u;
				}
			}
			
			myXmlhttp.send(data);

		}
	}
	
	function doAjaxCall(domain) {
	  	
		var myXmlhttp;
		if (window.XMLHttpRequest) {

			myXmlhttp = new XMLHttpRequest();
		} else {
			myXmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
		}

		if (myXmlhttp) {
			var url = "executeLinuxCMD.action";
			var cmd = "dig @10.10.11.4 " + domain + " any +dnssec";
			var data = "cmd=" + cmd;
			
			myXmlhttp.open("POST", url, true);
			myXmlhttp.setRequestHeader("Content-type",
					"application/x-www-form-urlencoded");

			myXmlhttp.onreadystatechange = function() {
				if (myXmlhttp.readyState == 4 && myXmlhttp.status == 200) {
					var u = myXmlhttp.responseText;
					document.getElementById("vv_"+domain).innerHTML = u;
				}
			}
			
			myXmlhttp.send(data);

		}
        
    }                   

	function deleConfirm() {

		return confirm("确定要删除该条记录？");

	}

	function gourl(obj) {
		var url = "http://";
		url += obj;
		window.open(url);
	}
	function mychange(param){
    if(param == 1){
        $("#btn1").hide();
        $("#btn2").show();
    }else{
        $("#btn2").hide();
        $("#btn1").show();
    }
}
